Tab:AddButton({
 Name = "Infinity Strike",
 Callback = function()
        local ohCFrame1 = CFrame.new(160.584854, 27.395155, -108.068542, -0.276732266, -0.547994375, 0.789380372, 1.10285747e-09, 0.821460843, 0.570264876, -0.960947096, 0.157810688, -0.227324709)
