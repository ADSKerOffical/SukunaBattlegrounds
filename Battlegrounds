h:Destroy()
end)
   end    
})

local function AntiKick()
local plra = game:GetService("Players").LocalPlayer
getgenv().Anti = true
local Anti
Anti = hookmetamethod(game, "__namecall", function(self, ...)
        if self == plra and getnamecallmethod():lower() == "kick" and getgenv().Anti then
            return warn("[ANTI-KICK] Client Tried To Call Kick Function On LocalPlayer")
        end
        return Anti(self, ...)
    end)
end

AntiKick()
